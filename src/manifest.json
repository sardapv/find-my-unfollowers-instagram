{
  "manifest_version": 2,
  "name": "Find My Insta Unfollowers",
  "homepage_url": "http://localhost:8080/",
  "description": "Simple extension that helps filter list of who is not your follower but you follow them OR they follow you but you don't ðŸ˜ˆ",
  "default_locale": "en",
  "permissions": ["<all_urls>", "*://*/*"],
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "background": {
    "scripts": ["js/background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Find My Insta Unfollowers",
    "default_icon": {
      "19": "icons/19.png",
      "38": "icons/38.png"
    }
  }
}
